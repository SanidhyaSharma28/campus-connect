generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id        Int      @id @default(autoincrement())
  date      DateTime  
  company   String
  SPOC      String
  Process   String
  Timings   String
  Mode      String
  Branches  String
  Cutoff    Float
  Profile   String
  createdAt DateTime @default(now())
}
model Resource {
  id        Int      @id @default(autoincrement())
  createdBy String
  company   String
}
